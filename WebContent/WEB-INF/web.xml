<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>BankWeb</display-name>
  <error-page>
    <error-code>404</error-code>
    <location>/WEB-INF/jsp/errors/err404.jsp</location>
  </error-page>
  <welcome-file-list>
    <welcome-file>index.do</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>BankServ</servlet-name>
    <servlet-class>lu.serv.BankServ</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BankServ</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <include-prelude>/WEB-INF/jsp/includes/taglib.jsp</include-prelude>
    </jsp-property-group>
  </jsp-config>
</web-app>